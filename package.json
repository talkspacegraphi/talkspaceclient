{
  "name": "talkspace",
  "version": "1.0.4",
  "private": false,
  "main": "electron.js",
  "homepage": "./",
  "description": "TalkSpace Messenger Ultimate",
  "author": "Graphi",
  "dependencies": {
    "axios": "^1.6.0",
    "electron-log": "^5.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.0",
    "emoji-picker-react": "^4.9.2",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.344.0",
    "open-graph-scraper": "^6.5.0",
    "peerjs": "^1.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^4.0.0",
    "socket.io-client": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "electron": "^29.1.0",
    "electron-builder": "^24.13.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron-dev": "electron .",
    "dist": "react-scripts build && electron-builder"
  },
  "build": {
    "appId": "com.talkspace.app",
    "productName": "TalkSpace",
    "files": ["build/**/*", "electron.js"],
    "directories": { "output": "dist" },
    "win": { "target": ["squirrel"], "icon": "public/favicon.ico" }
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  }
}